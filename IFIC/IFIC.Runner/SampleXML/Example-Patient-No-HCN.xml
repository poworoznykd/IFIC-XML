  </text>
  <!--  Sex at birth  -->
  <extension
             url="http://cihi.ca/fhir/irrs/StructureDefinition/irrs-ext-birth-sex">
    <valueCode value="F"/>
  </extension>
  <!--  Health card number that is unknown -->
  <identifier>
    <extension
               url="http://cihi.ca/fhir/irrs/StructureDefinition/irrs-ext-data-absent-reason">
      <valueCode value="unknown"/>
    </extension>
    <type>
      <coding>
        <system value="http://hl7.org/fhir/v2/0203"/>
        <code value="JHN"/>
      </coding>
    </type>
  </identifier>
  <!--  Case record number  -->
  <identifier>
    <type>
      <coding>
        <system value="http://hl7.org/fhir/v2/0203"/>
        <code value="MR"/>
      </coding>
    </type>
    <system value="http://acme.vendor.com/facility-x"/>
    <value value="CRN-12345"/>
  </identifier>
  <!--  Birth date  -->
  <birthDate value="2008-09-01"/>
  <!--  Postal Code  -->
  <address>
    <use value="home"/>
    <postalCode value="Z1Z1Z1"/>
  </address>
  <!--  Primary Language  -->
  <communication>
    <language>
      <coding>
        <code value="en"/>
      </coding>
    </language>
  </communication>
  <!--  Facility/Agency identifier  -->
  <managingOrganization>
    <identifier>
      <system
              value="http://cihi.ca/fhir/NamingSystem/on-moh-submission-identifier"/>
      <value value="5009"/>
