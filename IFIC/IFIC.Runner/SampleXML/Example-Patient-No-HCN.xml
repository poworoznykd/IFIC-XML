      <table class="grid">
        <tr>
          <td>-</td>
          <td>
            <b>Language</b>
          </td>
        </tr>
        <tr>
          <td>*</td>
          <td>
            en <span style="background: LightGoldenRodYellow">(Details : {[not stated] code 'en' = 'en)</span>
          </td>
        </tr>
      </table>
      <p>
        <b>managingOrganization</b>:
      </p>
